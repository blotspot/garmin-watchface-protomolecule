<layout id="WatchFace">
	<drawable id="Background" class="Background"/>

  <drawable id="DateAndTime" class="DateAndTime">
    <param name="burnInProtectionMode">false</param>
  </drawable>

  <drawable id="NoProgressDataField1" class="SecondaryDataField">
    <param name="fieldId">FieldId.NO_PROGRESS_1</param>
    <param name="offsetModifier">0</param>
    <param name="x">0.168 * dc.getWidth()</param>
    <param name="y">0.685 * dc.getHeight()</param>
  </drawable>
  <drawable id="NoProgressDataField2" class="SecondaryDataField">
    <param name="fieldId">FieldId.NO_PROGRESS_2</param>
    <param name="offsetModifier">0.5</param>
    <param name="x">0.5 * dc.getWidth()</param>
    <param name="y">0.685 * dc.getHeight()</param>
  </drawable>
  <drawable id="NoProgressDataField3" class="SecondaryDataField">
    <param name="fieldId">FieldId.NO_PROGRESS_3</param>
    <param name="offsetModifier">1</param>
    <param name="x">0.832 * dc.getWidth()</param>
    <param name="y">0.685 * dc.getHeight()</param>
  </drawable>

  <drawable id="OuterDataField" class="OrbitDataField">
    <param name="fieldId">FieldId.ORBIT_OUTER</param>
    <param name="startDegree">270</param>
    <param name="totalDegree">360</param>
    <param name="radius">dc.getWidth() / 2.0 * 0.96</param>
  </drawable>

  <drawable id="UpperDataField1" class="OrbitDataField">
    <param name="fieldId">FieldId.ORBIT_LEFT</param>
    <param name="startDegree">248</param>
    <param name="totalDegree">150</param>
    <param name="radius">dc.getWidth() / 2.0 * 0.89</param>
  </drawable>

  <drawable id="UpperDataField2" class="OrbitDataField">
    <param name="fieldId">FieldId.ORBIT_RIGHT</param>
    <param name="startDegree">82</param>
    <param name="totalDegree">150</param>
    <param name="radius">dc.getWidth() / 2.0 * 0.89</param>
  </drawable>
</layout> 